<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="me.twammer.android"
      android:versionCode="1"
      android:versionName="1.0">
    <application android:icon="@drawable/twam_logo" android:label="@string/app_name">
        <activity android:name=".TwammerActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
         <activity android:label="@string/app_name" android:name="TwamListActivity">
            <intent-filter>
                <action android:name="me.twammer.android.intent.action.SHOWLIST" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>
    <uses-sdk android:minSdkVersion="4" />

<uses-permission android:name="android.permission.INTERNET"></uses-permission>
</manifest> 